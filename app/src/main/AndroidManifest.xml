<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aimscapstone.infectionpreventionsource" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TrainingNavigationActivity"
            android:label="@string/title_activity_training_navigation" >
        </activity>
        <activity
            android:name=".VideoNavigationActivity"
            android:label="@string/title_activity_video_navigation" >
        </activity>
        <activity
            android:name=".VideoActivity"
            android:label="@string/title_activity_video" >
        </activity>
        <activity
            android:name=".ResourcesActivity"
            android:label="@string/title_activity_resources" >
        </activity>
        <activity
            android:name=".FaqActivity"
            android:label="@string/title_activity_faq" >
        </activity>
    </application>

</manifest>
